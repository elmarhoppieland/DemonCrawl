; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="DemonCrawl"
run/main_scene="uid://duasdk788nmel"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Tooltip="res://engine/resources/singletons/tooltip.tscn"
Focus="res://engine/resources/singletons/focus.tscn"
Debug="res://engine/resources/singletons/debug.tscn"
Toasts="res://engine/resources/singletons/toasts.tscn"
DCPopup="res://engine/resources/scenes/dc_popup.tscn"
GuiLayer="res://engine/resources/singletons/gui_layer.tscn"

[debug]

gdscript/warnings/exclude_addons=false
gdscript/warnings/integer_division=0
gdscript/warnings/confusable_local_declaration=0

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="viewport"
mouse_cursor/custom_image="res://assets/sprites/cursor.png"

[editor]

script/search_in_file_extensions=PackedStringArray("gd", "gdshader", "tscn", "tres", "uid")
naming/scene_name_casing=1
naming/script_name_casing=1

[editor_plugins]

enabled=PackedStringArray("res://addons/auto_thread/plugin.cfg", "res://addons/custom_texture_preview/plugin.cfg", "res://addons/dc_plugin/plugin.cfg", "res://addons/effect_manager/plugin.cfg", "res://addons/icon_manager/plugin.cfg", "res://addons/immunity/plugin.cfg", "res://addons/promise/plugin.cfg")

[eternity]

named_paths/defaults={
"settings": "user://settings.ini"
}
editor/editor_save_path="res://editor_assets/editor_save.ini"

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://assets/": "purple",
"res://editor_assets/": "orange",
"res://effect_manager/": "gray",
"res://engine/": "teal"
}

[filesystem]

import/blender/enabled=false

[gui]

theme/default_font_antialiasing=0
theme/custom="uid://b442tpxg2qr7y"
theme/custom_font="uid://7ynms6dclfsi"

[importer_defaults]

csv_translation={
"delimiter": 1
}

